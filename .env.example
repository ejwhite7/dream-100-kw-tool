# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Database encryption key for API keys (use a strong 32+ character key)
DATABASE_ENCRYPTION_KEY=your-super-secure-encryption-key-here-32chars-min

# External API Keys (will be encrypted and stored in database per user)
# These are fallback/system keys - users can provide their own
AHREFS_API_KEY=your-ahrefs-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Rate Limiting & Performance
MAX_KEYWORDS_PER_RUN=10000
MAX_CONCURRENT_API_REQUESTS=10
API_TIMEOUT_MS=30000

# Redis Cache Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
# For Redis Cluster (optional)
REDIS_CLUSTER_NODES=

# Cache TTL Settings (milliseconds)
AHREFS_CACHE_TTL=2592000000
ANTHROPIC_CACHE_TTL=86400000
EMBEDDING_CACHE_TTL=604800000
SERP_CACHE_TTL=604800000
CLUSTER_CACHE_TTL=86400000
SCORE_CACHE_TTL=21600000
EXPORT_CACHE_TTL=3600000

# Cache Features
CACHE_COMPRESSION_ENABLED=true
CACHE_COMPRESSION_THRESHOLD=1024
CACHE_MONITORING_ENABLED=true
CACHE_STATS_INTERVAL=60000
CACHE_WARMING_ENABLED=true
CACHE_DISTRIBUTED_LOCKING=true
CACHE_BATCH_OPERATIONS=true

# Cache Performance
CACHE_MAX_KEY_LENGTH=250
CACHE_CONNECTION_TIMEOUT=10000
CACHE_COMMAND_TIMEOUT=5000

# Legacy Caching Configuration (deprecated)
METRICS_CACHE_TTL_DAYS=30
COMPETITOR_CACHE_TTL_DAYS=7

# Monitoring & Logging (Optional)
SENTRY_DSN=your-sentry-dsn
POSTHOG_KEY=your-posthog-key
LOG_LEVEL=info

# CSV Export Settings
MAX_EXPORT_ROWS=50000
EXPORT_CACHE_TTL_HOURS=1