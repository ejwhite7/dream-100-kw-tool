# Local Development Environment Variables
# Copy this file to .env.local for local development

# Supabase Configuration (Development)
NEXT_PUBLIC_SUPABASE_URL=https://your-dev-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-dev-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-dev-service-role-key

# Development Security Keys
DATABASE_ENCRYPTION_KEY=dev-encryption-key-32chars-minimum
JWT_SECRET=dev-jwt-secret
NEXTAUTH_SECRET=dev-nextauth-secret
NEXTAUTH_URL=http://localhost:3000

# External API Keys (Development/Test)
AHREFS_API_KEY=your-dev-ahrefs-api-key
ANTHROPIC_API_KEY=your-dev-anthropic-api-key

# Local Redis (optional)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Development Settings
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=Dream 100 Keyword Engine (Dev)
NEXT_PUBLIC_APP_VERSION=1.0.0-dev

# Development Features
DEBUG=false
LOG_LEVEL=debug
CACHE_ENABLED=true
MOCK_EXTERNAL_APIS=false

# Optional Development Services
SENTRY_DSN=
NEXT_PUBLIC_POSTHOG_KEY=
SLACK_WEBHOOK_URL=

# Bundle Analysis
ANALYZE=false

# Performance Testing
DISABLE_RATE_LIMITING=true
FAST_REFRESH_ENABLED=true
